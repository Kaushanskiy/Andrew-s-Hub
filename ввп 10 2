#include <stdio.h> //Подключение библиотеки ввода/вывода   
#include <string.h>   
#include <math.h> //Подключение библиотеки математических функций   
#include <locale.h>   
//Точка входа в программу - главная функция   
int main() {   
    setlocale(LC_ALL, "Russian");
    int A,B,C;
//Даны три целых числа: A, B, C. Проверить истинность высказывания:
//«Справедливо двойное неравенство A < B < C».

    printf ("Введите числа A, B и C  \n");   
    scanf ("%d/t", &A);
    scanf ("%d/t", &B); 
    scanf ("%d/t", &C); 
            if ((A < B) && (B < C)){
       //Вывод результатов   
    printf("Справедливо двойное неравенство A < B < C");}
            else{    printf("Двойное неравенство A < B < C не справедливо");}        
    
            return 0;   
}
