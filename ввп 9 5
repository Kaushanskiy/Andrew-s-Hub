#include <stdio.h> //Подключение библиотеки ввода/вывода    
#include <string.h>    
#include <math.h> //Подключение библиотеки математических функций    
#include <locale.h>    
//Точка входа в программу - главная функция    
int main() {    
    setlocale(LC_ALL, "Russian"); 
    int god; 
//Дан номер некоторого года (целое положительное число).
//Определить соответствующий ему номер столетия, учитывая,
//что, к примеру, началом 20 столетия был 1901 год  
    printf ("Введите  K - номер года: ");    
    scanf ("%d/t", &god);  
//Вывод результатов   
           printf("Номер столетия:   %d", god/100 + 1) ; 
        return 0;    
}
