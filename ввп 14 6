#include <stdio.h> //Подключение библиотеки ввода/вывода   
#include <string.h>   
#include <math.h> //Подключение библиотеки математических функций   
#include <locale.h>   
//Точка входа в программу - главная функция   
int main() {   
    setlocale(LC_ALL, "Russian");

//Дано целое число N (> 1), являющееся числом Фибоначчи:
//N = FK. Найти целое число K — порядковый номер числа 
//Фибоначчи N.
  
	int n;
   printf("Введите целое число N:\n");
   scanf("%i",&n);
   int f1=1,f2=1,f=0,k=2;
 
   while (f<n){
       ++k;
       f=f2+f1;
       f2=f1;
       f1=f;
   }
   printf("Порядковый номер числа Фибоначчи K: %i\n",k);
   return 0;
}
