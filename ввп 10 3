#include <stdio.h> //Подключение библиотеки ввода/вывода   
#include <string.h>   
#include <math.h> //Подключение библиотеки математических функций   
#include <locale.h>   
//Точка входа в программу - главная функция   
int main() {   
    setlocale(LC_ALL, "Russian");
    int x;
//Дано целое положительное число. Проверить истинность высказывания:
//«Данное число является четным двузначным».

    printf ("Введите целое положительное число \n");   
    scanf ("%d/t", &x);
    if ((x%2 == 0) && (x > 9) && (x < 100)){
       //Вывод результатов   
    printf("Число x - чётное двухзначное");}
            else{    printf("Число x не является чётным двухзначным");}        
    
            return 0;   
}
