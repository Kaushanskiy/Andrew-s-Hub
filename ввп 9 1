#include <stdio.h> //Подключение библиотеки ввода/вывода   
#include <string.h>   
#include <math.h> //Подключение библиотеки математических функций   
#include <locale.h>   
//Точка входа в программу - главная функция   
int main() {   
    setlocale(LC_ALL, "Russian");
    int N, ost;
//С начала суток прошло N секунд (N — целое).
//Найти количество секунд, прошедших с начала последней минуты.
 
    printf ("Количество секунд N  ");   
    scanf ("%d/t", &N); 
            ost = N%60;
       //Вывод результатов   
    printf("Количество секунд, прошедших с начала последней минуты %d\n", ost);
            return 0;   
}
