#include <stdio.h> //Подключение библиотеки ввода/вывода    
#include <string.h>    
#include <math.h> //Подключение библиотеки математических функций    
#include <locale.h>    
//Точка входа в программу - главная функция    
int main() {    
    setlocale(LC_ALL, "Russian"); 
    int A, B, C;
    float d1, d2;
//Даны целые положительные числа A, B, C. На прямоугольнике размера
//A × B размещено максимально возможное количество квадратов со 
//стороной C (без наложений). Найти количество квадратов, 
//размещенных на прямоугольнике, а также площадь незанятой части
//прямоугольника. 
    printf ("Введите  стороны прямоугольника A и B: ");    
    scanf ("%d" "%d", &A, &B);
    printf ("Введите  сторону квадрата C: ");    
    scanf ("%d", &C);  
//Вывод результатов  
    d1 = ((float) (A/C))*((float) (B/C));
    d2 = A*B - C*C*d1; 
    printf("Количество квадратов, которые можно разместить   %4.1f\n", d1) ; 
    printf("Площадь незанятой части прямоугольника   %4.1f", d2) ; 
        return 0;    
}
